{
	"name": "Rust",
	"build": {
		"dockerfile": "alpine.Dockerfile"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"dbaeumer.vscode-eslint",
				"rust-lang.rust-analyzer",
				"ms-azuretools.vscode-docker",
				"svelte.svelte-vscode",
				"tamasfe.even-better-toml"
			]
		}
	},
	"forwardPorts": [
		8000
	]
}
